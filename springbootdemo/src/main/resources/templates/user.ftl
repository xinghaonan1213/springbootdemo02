<html>    <head>        <title> spring boot</title>    </head>    <body>        <table border="3px">            <thead>                <tr>                    <th>id</th>                    <th>账号</th>                    <th>密码</th>                    <th>名字</th>                </tr>            </thead>            <#list userList as user >                <tbody>                <tr>                    <td>${user.id}</td>                    <td>${user.username}</td>                    <td>${user.password}</td>                    <td>${user.name}</td>                </tr>                </tbody>            </#list>        </table>    </body></html>